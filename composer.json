{
    "name": "edouardtack/loaderyaml",
    "description": "LoaderYaml plugin for CakePHP. You can load yaml config files.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Edouard Tack",
            "email": "edouard@tackacoder.fr"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "LoaderYaml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoaderYaml\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
